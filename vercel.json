{
  "buildCommand": "cd fantasy-bball-app && npm run build",
  "outputDirectory": "fantasy-bball-app/.next",
  "devCommand": "cd fantasy-bball-app && npm run dev",
  "installCommand": "pip install -r requirements.txt && cd fantasy-bball-app && npm install",
  "framework": "nextjs",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
